      "type": "string"
    },
    "emailBody": {
      "type": "string",
      "mysql": {
        "dataType": "text"
      }
    },
    "sentDt": {
      "type": "date"
    },
    "mailgunMessageId": {
      "type": "string"
    },
    "wasSent": {
